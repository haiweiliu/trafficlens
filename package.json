{
  "name": "trafficlens",
  "version": "1.0.0",
  "description": "TrafficLens - Website Traffic Analytics Tool. Analyze traffic data for multiple domains with insights on visits, growth trends, and engagement metrics.",
  "main": "index.js",
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postinstall": "if [ -z \"$VERCEL\" ]; then npx playwright install chromium --with-deps; fi",
    "start": "next start",
    "lint": "next lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "qa": "tsx scripts/qa-agent.ts",
    "qa:daily": "bash scripts/run-qa.sh",
    "qa:loop": "tsx scripts/ralph-wiggum-agent.ts",
    "qa:loop:full": "tsx scripts/ralph-wiggum-agent.ts --full",
    "qa:loop:quick": "tsx scripts/ralph-wiggum-agent.ts --quick",
    "health:check": "tsx scripts/playwright-health-cron.ts",
    "health:cron:setup": "bash scripts/setup-health-cron.sh",
    "test:email": "tsx scripts/test-email.ts",
    "data:quality": "tsx scripts/data-quality-agent.ts",
    "pre-deploy": "tsx scripts/pre-deployment-agent.ts",
    "report:daily": "tsx scripts/daily-report.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sparticuz/chromium": "^143.0.4",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.4.7",
    "next": "^16.1.1",
    "nodemailer": "^7.0.12",
    "playwright": "^1.57.0",
    "playwright-core": "^1.57.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/dotenv": "^8.2.3",
    "@types/nodemailer": "^7.0.4",
    "tsx": "^4.21.0"
  }
}